programa
{
	
	funcao inicio()
	{
		real prof
		escreva("Digite a profundidade atual do submarino: ")
		leia (prof)
		se (prof > 1000) {
			escreva ("Esmagamento iminente!!")
}
  		senao se (prof > 500) {
  			escreva ("Alerta! Perigo de implodição!")
}
  		senao {
  			escreva ("Calm down, everything is goona be fine, just relax!")
  }
	}
}
