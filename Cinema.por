programa
{
	
	funcao inicio()
	{
		inteiro idade
		escreva("Digite sua idade:")
		leia (idade)
	se (idade < 10) {
		escreva ("Você pode assistir a um filme livre")
	}
	senao se (idade >= 10 e idade< 14) {
	     escreva ("Você pode assistir a um filme juvenil")
	}
     senao {
     	escreva ("Você pode assistir a um filme adulto")
     }
   }
  }

