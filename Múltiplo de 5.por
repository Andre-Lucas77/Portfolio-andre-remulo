programa
{
	
	funcao inicio()
	{
		inteiro num
	escreva ("Digite um número para eu checar se é múltiplo de 5:")
	leia (num)
	se (num % 5 == 0) {
	 escreva ("Acertouuu!")
	}
		senao {
			escreva ("Errouuu!")
		}
		
	}
}
