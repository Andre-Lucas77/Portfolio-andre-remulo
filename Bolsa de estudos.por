programa
{
	
	funcao inicio()
	{
		real nota
		real renda
		escreva("Qual sua renda mensal? ")
		leia (renda)
		escreva ("Qual a nota que você tirou? ")
		leia (nota)
	se (nota > 8 e renda <2000){
	    escreva ("Parabéns, você foi aprovado para ter uma bolsa!")	
	  }
	senao {
		escreva ("Infelizmente você não tem os requisistos para ter uma bolsa de estudos conosco.")
   }
  }
}
