programa
{
	
	funcao inicio()
	{
		inteiro nota
		escreva("Digite sua nota:")
		leia (nota)
		se (nota >= 6) {
			escreva ("Você passou!!!")
		}
	     senao { 
               escreva ("Você reprovou :(") 
	      }
	}
}
